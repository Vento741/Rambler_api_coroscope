# Настройки сервера
# HOST=0.0.0.0
# PORT=8000
# DEBUG=false
# WORKERS=4

# Настройки CORS
# ADDITIONAL_CORS_ORIGINS=https://yourdomain.com,https://app.yourdomain.com

# Настройки кэша
CACHE_TTL_MINUTES=30
CACHE_CLEANUP_INTERVAL=300

# Настройки парсера
PARSER_TIMEOUT=10
MAX_CONCURRENT_REQUESTS=100

# Настройки логирования
LOG_LEVEL=INFO

# Настройки для продакшн
PROD_MODE=true

# SSL настройки (опционально)
# SSL_CERT_PATH=/path/to/cert.pem
# SSL_KEY_PATH=/path/to/key.pem

# Настройки прокси (опционально)
# PROXY_PREFIX=api 

# Настройки для API openrouter
OPENROUTER_API_KEY_1=Ключ 1
OPENROUTER_API_KEY_2=Ключ 2
OPENROUTER_API_KEY_3=Ключ 3
URL_LINK_OPENROUTER=https://openrouter.ai/api/v1/chat/completions